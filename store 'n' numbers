#include <stdio.h>

int main(void) {
  int a[99],i,n;
  printf("enter the array size");
  scanf("%d",&a[i]);
  printf("enter array elements");
  for(i=0;i<n;i++)
    {
      scanf("%d",&a[i]);
      
    }
  printf("array elements are:");
  for(i=0;i<n;i++)
  {
    printf("%d\t",a[i]);
  }
  return 0;
}
